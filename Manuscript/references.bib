@article{minnaarPlantPollinatorInteractions2019,
  title = {Plant\textendash Pollinator Interactions along the Pathway to Paternity},
  author = {Minnaar, Corneile and Anderson, Bruce and {de Jager}, Marinus L and Karron, Jeffrey D},
  year = {2019},
  month = jan,
