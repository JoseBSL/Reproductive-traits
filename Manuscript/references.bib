@article{kaiser2014,
  title = {Determinants of the Microstructure of Plant\textendash Pollinator Networks},
  author = {{Kaiser-Bunbury}, Christopher N. and V{\'a}zquez, Diego P. and Stang, Martina and Ghazoul, Jaboury},
  year = {2014},
  journal = {Ecology},
  volume = {95},
